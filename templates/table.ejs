<% if (DATA.length===0) { %>
    <tr>
        <td class="is-narrow"></td>
        <td class="is-narrow"></td>
        <td class="is-narrow"></td>
        <td>(nada por el momento)</td>
    </tr>
<% } %>
<% DATA.forEach(function(reg,idx2){ %>
    <tr id="<%= 'r-'+(idx2.toString()) %>">
        <td class="p-1 is-narrow has-text-centered has-text-weight-bold"><%= reg.desde %> - <%= reg.hasta %></td>   <!-- horario -->

    <% if (show_pabellon) { %>
        <td class="p-1 is-narrow has-text-centered has-text-weight-bold">                                           <!-- pabellon -->
        <%= reg.pabellon %>
        </td>
    <% } %>

        <td class="py-1 is-narrow has-text-centered has-text-weight-bold">                                          <!-- aula -->
            <% if (reg.is_composite) { %>
                &rarr;
            <% } else { %>
                <%= reg.aula %>
            <% } %>
        </td>

        <td class="p-1 is-narrow <%= reg.classcolor %>"> </td>                                                      <!-- estado -->

        <td class="p-1 is-fullwidth mx-2">                                                                               <!-- materia -->
            <div class="block is-float-left mx-1 my-0">
                <% reg.materia.forEach(function(namep, idx) { %>
                    <% if (idx!=0) {%> <br/> <% } %>
                    <%= namep %>
                <% }) %>
            </div>
            <% if (reg.is_composite) { %>
                <% reg.extra.forEach(function(aula_sub) { %>
                <span class="pill has-text-weight-bold">
                    <%= aula_sub[0] %><% if (aula_sub[1].trim()!=="") { %>: <%= aula_sub[1] %> <% } %>
                </span>
                <% }) %>
            <% } %>
        </td>

    </tr>
<% }) %>
